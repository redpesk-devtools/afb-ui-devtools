{
  "name": "afb-ui-devtools",
  "version": "1.0.4",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redpesk-devtools/afb-ui-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/redpesk-devtools/afb-ui-devtools/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint afb-ui-devtools --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "pack": "PKG_NAME=$npm_package_name-$npm_package_version; rm -rf ./dist && npm run lint:ci && npm run build:prod && mv dist $PKG_NAME && tar zcvf $PKG_NAME.tar.gz ./$PKG_NAME/"
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^10.1.1",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "^10.2.1",
    "@angular/compiler": "^10.1.1",
    "@angular/core": "^10.2.1",
    "@angular/forms": "^10.1.1",
    "@angular/localize": "^10.1.1",
    "@angular/platform-browser": "^10.1.1",
    "@angular/platform-browser-dynamic": "^10.1.1",
    "@angular/router": "^10.1.1",
    "@nebular/eva-icons": "^6.0.1",
    "@nebular/theme": "^6.0.1",
    "bootstrap": "4.3.1",
    "chokidar": "^3.4.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "eslint": "^7.8.1",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "jquery": "^3.5.1",
    "nebular-icons": "1.1.0",
    "node-sass": "^4.14.1",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "popper.js": "^1.16.0",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.3.0",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.1.1",
    "@angular/language-service": "10.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "0.13.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "4.0.2",
    "protractor": "~7.0.0",
    "rimraf": "2.6.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "3.2.2",
    "typescript": "^3.9.7"
  }
}
