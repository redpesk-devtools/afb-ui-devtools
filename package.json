{
  "name": "afb-ui-devtools",
  "version": "1.0.5",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redpesk-devtools/afb-ui-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/redpesk-devtools/afb-ui-devtools/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "build": "ng build",
    "watch": "ng build --watch --aot",
    "build:prod": "ng build --configuration production --aot",
    "build:tar": "VERSION=$(git describe --always --tags --match \"[0-9]*\" --abbrev=0) && ng build --configuration production --aot && mkdir -p afb-ui-devtools-prebuild-$VERSION && mv dist/* afb-ui-devtools-prebuild-$VERSION/ && tar -czf afb-ui-devtools-prebuild-$VERSION.tar.gz afb-ui-devtools-prebuild-$VERSION && rm -rf afb-ui-devtools-prebuild-$VERSION",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint afb-ui-devtools --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "pack": "PKG_NAME=$npm_package_name-$npm_package_version; rm -rf ./dist && npm run lint:ci && npm run build:prod && mv dist $PKG_NAME && tar zcvf $PKG_NAME.tar.gz ./$PKG_NAME/"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@nebular/eva-icons": "^12.0.0",
    "@nebular/theme": "^12.0.0",
    "bootstrap": "4.3.1",
    "chokidar": "^3.4.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "jquery": "^3.5.1",
    "nebular-icons": "1.1.0",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "popper.js": "^1.16.0",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.3.0",
    "sass": "^1.77.0",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "16.2.12",
    "@types/estree": "^1.0.5",
    "@types/node": "^14.14.31",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.51.0",
    "husky": "0.13.3",
    "npm-run-all": "4.0.2",
    "protractor": "~7.0.0",
    "rimraf": "2.6.1",
    "ts-node": "3.2.2",
    "typescript": "^4.6.4"
  }
}